{
  "name": "tarsus",
  "version": "1.4.0",
  "description": "new ts-decorator for express server",
  "repository": {
    "url": "https://github.com/chelizichen/tarsus",
    "type": "git"
  },
  "keywords": [
    "express",
    "ts",
    "decorator",
    "ioc"
  ],
  "exports": {

    "./httpservice": {
      "require": "./dist/httpservice.js",
      "import": "./dist/httpservice.mjs",
      "types": "./dist/types/httpservice.d.ts"
    },
    "./microservice": {
      "require": "./dist/microservice.js",
      "import": "./dist/microservice.mjs",
      "types": "./dist/types/microservice.d.ts"
    },
    "./ioc": {
      "require": "./dist/ioc.js",
      "import": "./dist/ioc.mjs",
      "types": "./dist/types/ioc.d.ts"
    },
    "./util": {
      "require": "./dist/util.js",
      "import": "./dist/util.mjs",
      "types": "./dist/types/util.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "http": "ts-node-dev test/http/index.ts",
    "server": "ts-node-dev test/microservice/index.ts",
    "build": " tsc --project tsconfig.build.json --format cjs,esm",
    "pkg": "tsc --project tsconfig.pkg.json && tsup lib --format cjs,esm",
    "test:proxy": "ts-node-dev lib/web/proxy/TarsusRequest.ts",
    "servant": "ts-node-dev lib/util/servant.ts",
    "orm": "ts-node-dev test/http/entity/goods.entity.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "axios": "^1.3.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "redis": "^4.3.1",
    "tarsus-cli": "^1.2.94",
    "ts-node-dev": "^2.0.0-0",
    "typescript": "^5.2.1-rc",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/mysql": "^2.15.21"
  }
}
