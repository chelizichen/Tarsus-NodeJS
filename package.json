{
  "name": "tarsus",
  "version": "1.3.1",
  "description": "new ts-decorator for express server",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "url": "https://github.com/chelizichen/tarsus",
    "type": "git"
  },
  "keywords": [
    "express",
    "ts",
    "decorator",
    "ioc"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./web": {
      "require": "./dist/web.js",
      "import": "./dist/web.mjs",
      "types": "./dist/types/web.d.ts"
    },
    "./ms": {
      "require": "./dist/ms.js",
      "import": "./dist/ms.mjs",
      "types": "./dist/types/ms.d.ts"
    },
    "./util": {
      "require": "./dist/util.js",
      "import": "./dist/util.mjs",
      "types": "./dist/types/util.d.ts"
    },
    "./cache": {
      "require": "./dist/cache.js",
      "import": "./dist/cache.mjs",
      "types": "./dist/types/cache.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "http": "ts-node-dev test/http/index.ts",
    "server": "ts-node-dev test/microservice/index.ts",
    "build": " tsc --project tsconfig.build.json --format cjs,esm",
    "pkg": "tsc --project tsconfig.pkg.json && tsup decorator --format cjs,esm",
    "test:proxy": "ts-node-dev decorator/web/proxy/TarsusRequest.ts",
    "servant": "ts-node-dev decorator/util/servant.ts",
    "orm":"ts-node-dev test/http/entity/goods.entity.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "redis": "^4.3.1",
    "tarsus-cli": "^1.0.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.0.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/mysql": "^2.15.21",
    "@types/react": "18.0.28"
  }
}
