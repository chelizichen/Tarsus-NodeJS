    "esModuleInterop":true,
    "types": ["node"],
    "lib": ["es2022", "ESNext","ES2015"],
    "declaration":true,
    "declarationDir": "types",
    "target": "ES5",
    "emitDeclarationOnly": true
  },
  "include": [
    "./lib/*",
  ],
  "exclude": [
    "./test/*"
  ]
}